{
  "manifest_version": 3,
  "name": "GPT-Translate",
  "description": "ChatGPTで文章を翻訳します",
  "version": "1.1",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/icons/16.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}
